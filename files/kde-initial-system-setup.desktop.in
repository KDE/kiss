[Desktop Entry]
Exec=${CMAKE_INSTALL_FULL_LIBEXECDIR}/kde-initial-system-setup
NoDisplay=true
Type=Application
Name=KDE Initial System Setup
Name[ca]=Configuració inicial del sistema KDE
Name[ca@valencia]=Configuració inicial del sistema KDE
Name[eu]=KDE sistemaren hasierako ezarpena
Name[nl]=KDE initiële systeemopzet
Name[pt_BR]=Configuração inicial do sistema
Name[sl]=Začetna namestitev sistema KDE
Name[uk]=Початкове налаштовування системи KDE
Name[x-test]=xxKDE Initial System Setupxx
Comment=Run the initial system setup for KDE
Comment[ca]=Executa la configuració inicial del sistema KDE
Comment[ca@valencia]=Executa la configuració inicial del sistema KDE
Comment[eu]=Exekutatu KDE sistemaren hasierako ezarpena
Comment[nl]=Voer de initiële systeemopzet voor KDE uit
Comment[pt_BR]=Executar a configuração inicial do sistema
Comment[sl]=Zažene začetno namestitev sistema za KDE
Comment[uk]=Виконати початкове налаштовування системи для KDE
Comment[x-test]=xxRun the initial system setup for KDExx
Icon=kde
AutostartCondition=unless-exists kde-initial-setup-done
X-KDE-StartupNotify=false
